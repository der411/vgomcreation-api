{
  "kind": "collectionType",
  "collectionName": "projets",
  "info": {
    "singularName": "projet",
    "pluralName": "projets",
    "displayName": "Projets",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "titre": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "richtext",
      "required": true
    },
    "url_projet": {
      "type": "string",
      "unique": true
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "vendable": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "price": {
      "type": "decimal",
      "default": 0,
      "required": false
    },
    "priceId": {
      "type": "string",
      "required": false,
      "unique": true
    },
    "dateSold": {
      "type": "datetime",
      "required": false
    },
    "receiptUrl": {
      "type": "string",
      "required": false
    },
    "buyer_email": {
      "type": "email",
      "required": false,
      "private": true
    },
    "buyer_name": {
      "type": "string",
      "required": false,
      "private": true
    },
    "payment_status": {
      "type": "enumeration",
      "enum": [
        "pending",
        "completed",
        "failed",
        "refunded"
      ],
      "default": "pending"
    },
    "transaction_id": {
      "type": "string",
      "unique": true,
      "required": false
    },
    "purchase_metadata": {
      "type": "json",
      "required": false,
      "private": true
    },
    "sales_count": {
      "type": "integer",
      "default": 0,
      "required": true
    },
    "statut": {
      "type": "enumeration",
      "enum": [
        "En développement",
        "En attente",
        "Terminé"
      ]
    },
    "progression": {
      "type": "decimal"
    },
    "date_debut": {
      "type": "date"
    },
    "date_fin_prevue": {
      "type": "date"
    },
    "users_permissions_user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "projets"
    },
    "facture": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::facture.facture",
      "inversedBy": "projets"
    },
    "paiements": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::paiement.paiement",
      "mappedBy": "projet"
    },
    "categorie": {
      "type": "enumeration",
      "enum": [
        "E-commerce",
        "Education",
        "Restauration",
        "Finance",
        "Santé",
        "Médias",
        "Portfolio",
        "Blog",
        "Vitrine",
        "Immobilier",
        "Autre"
      ],
      "default": "Autre"
    }
  }
}
